{
  "name": "goodthing",
  "description": "For making new Preact projects with es modules and no compilation",
  "version": "1.0.0",
  "author": "Kynan Stewart",
  "bugs": {
    "url": "https://github.com/k7n4n5t3w4rt/goodthing/issues"
  },
  "dependencies": {
    "finalhandler": "^1.1.2",
    "history": "^4.10.1",
    "htm": "^3.0.4",
    "immer": "^7.0.7",
    "preact": "^10.4.1",
    "preact-router": "^3.2.1",
    "simplestyle-js": "^2.3.0-beta.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "cypress": "^4.11.0",
    "esinstall": "^1.1.7",
    "faucet": "0.0.1",
    "fixpack": "^3.0.6",
    "flow-bin": "^0.156.0",
    "glob": "^7.1.6",
    "prettier": "^2.0.0",
    "should": "^13.2.3"
  },
  "scripts": {
    "start": "browser-sync start --server",
